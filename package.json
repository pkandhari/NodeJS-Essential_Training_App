{
  "name": "nodejs-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build:development:ringcentral": "ng build -c=\"development, ringcentral\" --extra-webpack-config webpack.partial.js",
    "build:development:tenfold": "ng build -c=\"development, tenfold\" --extra-webpack-config webpack.partial.js",
    "build:qa:ringcentral": "ng build -c=\"qa, ringcentral\" --extra-webpack-config webpack.partial.js",
    "build:qa:tenfold": "ng build -c=\"qa, tenfold\" --extra-webpack-config webpack.partial.js",
    "build:production:ringcentral": "ng build -c=\"production, ringcentral\" --extra-webpack-config webpack.partial.js",
    "build:production:tenfold": "ng build -c=\"production, tenfold\" --extra-webpack-config webpack.partial.js",
    "serve:dev:ringcentral": "ng serve -c=\"development,ringcentral\" --extra-webpack-config webpack.partial.js -o",
    "serve:dev:tenfold": "ng serve -c=\"development,tenfold\" --extra-webpack-config webpack.partial.js -o",
    "coverage": "npm run test:unit-coverage && npm run test:coverage",
    "test:coverage": "cat ./coverage/lcov.info | coveralls",
    "lint:tslint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:stylelint": "stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
    "lint": "npm run lint:tslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:tslint -- --fix && npm run lint:stylelint -- --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@ng-select/ng-select": "^8.1.1",
    "@ngneat/until-destroy": "^8.1.4",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@ringcentral/sdk": "^4.7.1",
    "lodash": "^4.17.21",
    "ngx-build-plus": "^12.0.1",
    "postcss-scss": "^4.0.3",
    "qs": "^6.10.3",
    "rxjs": "7.4.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.41",
    "@types/qs": "^6.9.7",
    "copy-webpack-plugin": "^10.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-with-warnings": "^0.2.0",
    "eslint-plugin-import": "^2.25.3",
    "jasmine-core": "~3.7.0",
    "jest": "^27.4.7",
    "jest-create-mock-instance": "^2.0.0",
    "jest-environment-jsdom-global": "^3.0.0",
    "jest-preset-angular": "^11.0.1",
    "postcss": "^8.4.14",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.3"
  }
}
